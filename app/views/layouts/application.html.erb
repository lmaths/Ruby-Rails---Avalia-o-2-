    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav">
    <%= link_to 'Inicio', root_path, class: "btn btn-outline-danger"  %>
    <%= link_to 'Ver personagens', personagens_path, class: "btn btn-outline-danger"  %>
    <%= link_to 'Novo personagem', new_personagem_path, class: "btn btn-outline-danger"  %>
